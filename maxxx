import pandas as pd

# Define your data
data = {
    "95% Confidence Interval, Lower Limit": [15.30, 17.10, 17.00, 17.20],
    "95% Confidence Interval, Upper Limit": [17.70, 20.10, 19.10, 19.20],
    "Number of Responses": [1434, 1154, 1578, 1563],
    "Total Number of Survey Respondents": [8715, 6897, 9282, 8674],
}

# Create a DataFrame
df = pd.DataFrame(data)

# Save as CSV
df.to_csv("survey_data.csv", index=False)

print("CSV file created successfully!")
